<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane xmlns = "http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="mx.edu.utez.demo3.controller.AlumnoFormController"
            prefHeight="400.0" prefWidth="600.0" stylesheets="@../css/alumno_form.css">


    <VBox fx:id="formAlumno" alignment="CENTER" spacing="15">
        <VBOx fx:id="modalBox" styleClass="modal-box" spacing="15">
            <TextField fx:id="txtNombres" promptext="Ingresa el nombre" styleClass="form-field"/>        
            <TextField fx:id="txtApellidos" promptext="Ingresa el apellido" styleClass="form-field"/>        
            <TextField fx:id="txtCorreo" promptext="Ingresa el correo" styleClass="form-field"/>        
            <TextField fx:id="dpFechaNacimiento" promptext="Selecciona la fecha de nacimiento" styleClass="form-field"/>        
            <TextField fx:id="cbCarrera" promptext="Selecciona la carrera" styleClass="form-field"/>        

            <HBox alignment="CENTER" spacing="15">
<Button fx:id="btnCrear" styleClass="form-btn" text="Crear"/>
<Button fx:id="btnCancelar" styleClass="form-btn-cancel" text="Cancelar"/>
</HBox>
</VBox>
</VBox>
                